{
    "servers": {
        "jira-template": {
            "type": "stdio",
            "command": "dotnet",
            "args": [
                "run",
                "--project",
                "file path to project",
                "--no-build"
            ],
            "env": {
                "JIRA_HOST": "https://*.atlassian.net",
                "JIRA_USER": "<JIRA_USER>",
                "JIRA_TOKEN": "<JIRA_TOKEN>"
            }
        }
    }
}